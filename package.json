{
  "name": "ai-snippet-manager",
  "version": "1.0.0",
  "description": "AI-powered code snippet manager with automatic descriptions, explanations, and tagging",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "dev:test": "dotenv -e .env.test -- astro dev",
    "test:e2e": "node scripts/run-e2e-tests.js",
    "test:e2e:ui": "node scripts/run-e2e-tests.js --ui",
    "test:e2e:validate": "tsx tests/e2e/scripts/validate-env.ts",
    "test:e2e:check": "tsx tests/e2e/scripts/test-connection.ts",
    "test:e2e:setup": "npm run test:e2e:validate && npm run test:e2e:check"
  },
  "dependencies": {
    "@astrojs/react": "^4.0.0",
    "@astrojs/tailwind": "^6.0.0",
    "@astrojs/vercel": "^8.0.0",
    "@supabase/supabase-js": "^2.46.1",
    "astro": "^5.0.3",
    "openai": "^4.73.0",
    "prismjs": "^1.30.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.14",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.1",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.15.0",
    "eslint-plugin-astro": "^1.3.0",
    "jsdom": "^24.1.3",
    "prettier": "^3.4.1",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vitest": "^4.0.6"
  },
  "keywords": [
    "code-snippets",
    "ai",
    "developer-tools",
    "astro",
    "react",
    "supabase",
    "openai"
  ],
  "author": "",
  "license": "MIT"
}
