{
  "name": "ai-snippet-manager",
  "version": "1.0.0",
  "description": "AI-powered code snippet manager with automatic descriptions, explanations, and tagging",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,css}\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@astrojs/react": "^4.0.0",
    "@astrojs/tailwind": "^6.0.0",
    "@astrojs/vercel": "^8.0.0",
    "@supabase/supabase-js": "^2.46.1",
    "astro": "^5.0.3",
    "openai": "^4.73.0",
    "prismjs": "^1.29.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.10.1",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-plugin-astro": "^1.3.0",
    "prettier": "^3.4.1",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "code-snippets",
    "ai",
    "developer-tools",
    "astro",
    "react",
    "supabase",
    "openai"
  ],
  "author": "",
  "license": "MIT"
}
